use std::env;
use std::process;

fn usage(name: String){
    println!("Usage: {:?} url",name);
    process::exit(0x0100);
}

fn main() {
    let args: Vec<String> = env::args().collect();
    if args.len() != 2 {
        usage(args[0].clone());
    }
    qr2term::print_qr(args[1].clone()).unwrap()
}
